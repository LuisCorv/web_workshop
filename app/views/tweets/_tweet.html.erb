<div class="card">
  <div class="card-header">
    <%= tweet.user.full_name %>
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p>Message: <%= tweet.body %></p>
      <footer class="blockquote-footer">Date: <cite title="Source Title"><%= tweet.creation_date %></cite></footer>
      <footer class="blockquote-footer">Hashtags: <cite title="Source Title"><%= tweet.list_hashtags %></cite></footer>
      <%= link_to "Ver",tweet , class:"btn btn-primary" %>
      <%= link_to "Back",tweets_path , class:"btn btn-primary" %>
      
    </blockquote>
  </div>
</div>

<br>